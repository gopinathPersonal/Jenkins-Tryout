pipeline {
    agent any
  tools {
    maven 'maven-3.6.3' 
  }

    stages {
        stage('build') {
            steps {
                git 'https://github.com/gopinathPersonal/sampleJavaProject.git'
                sh 'mvn clean package'
                sh 'javac HelloWorldTest.java'
            }
        }
        stage('Run') {
            steps {
                sh 'java HelloWorldTest'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
